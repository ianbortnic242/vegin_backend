<table style="width:50%">
  <tr>
    <th id="id">ID</th>
    <th id="nameHeader">Name</th>
    <th id="PriceHeader">Price</th>
    <th id="ThumbnailHeader">Thumbnail</th>
  </tr>
  <% products.forEach(anItem=> { %>
    <tr>
      <td>
        <%= anItem.id %>
      </td>
      <td>
        <%= anItem.title %>
      </td>
      <td>
        <%= anItem.price %>
      </td>
      <td>
        <%= anItem.thumbnail %>
      </td>
    </tr>
    <% }) %>
</table>